<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="640.0" maxWidth="480.0" minHeight="640.0" minWidth="480.0" prefHeight="640.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.LeaderBoardController">
    <children>
        <TableView fx:id="leaderboard" layoutX="56.0" layoutY="33.0" prefHeight="527.0" prefWidth="367.0">
            <columns>
                <TableColumn id="rankKolom" maxWidth="1000.0" prefWidth="75.0" text="Rank">
                    <cellValueFactory>
                        <PropertyValueFactory property="rank" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn id="namesKolom" prefWidth="75.0" text="Naam">
                    <cellValueFactory>
                        <PropertyValueFactory property="naam" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn id="puntenKolom" prefWidth="75.0" text="Points">
                    <cellValueFactory>
                        <PropertyValueFactory property="points" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
      <Button fx:id="dbButton" layoutX="202.0" layoutY="585.0" mnemonicParsing="false" text="Dashboard" onAction="#onDBButtonClick" />
    </children>
</AnchorPane>
